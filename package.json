{
  "name": "vitepress-project",
  "private": true,
  "type": "module",
  "scripts": {
    "pre:version": "node ./scripts/fetch-butler-version.mjs",
    "dev": "npm run pre:version && vitepress dev docs",
    "build": "npm run pre:version && vitepress build docs",
    "serve": "npm run pre:version && vitepress serve docs",
    "deploy": "gh workflow run deploy-vitepress.yml --ref $(git branch --show-current)"
  },
  "devDependencies": {
    "mermaid": "^11.12.2",
    "ufo": "^1.6.1",
    "vitepress": "^1.6.4",
    "vitepress-plugin-mermaid": "^2.0.17",
    "vue": "^3.5.25"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search"
      ]
    }
  },
  "dependencies": {
    "medium-zoom": "^1.1.0",
    "swagger-ui-dist": "^5.30.3",
    "swagger-ui-react": "^5.30.3",
    "vitepress-openapi": "^0.1.13"
  }
}
